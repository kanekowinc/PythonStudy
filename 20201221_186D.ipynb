   "source": [
    "N = int(input())\n",
    "A = list(map(int,input().split()))\n",
    "\n",
    "A.sort(reverse=True)\n",
    "\n",
    "t = 0\n",
    "ans = 0\n",
    "for i in range(N - 1, 0, -1):\n",
    "    t += A[i]\n",
    "    print(t)\n",
    "    ans += A[i - 1] * (N - i) - t\n",
    "    print(ans)\n",
    "\n",
    "print(ans)\n"
   ]
