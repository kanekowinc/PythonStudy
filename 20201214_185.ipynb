    "N,M,T = map(int,input().split())\n",
    "a,b = [list(map(int,input().split())) for _ in range(M)]\n",
    "\n",
    "for m in range(M):\n",
    "    # バッテリー消費\n",
    "    N -= a[m]\n",
    "    if N < 0:\n",
    "        N = 0\n",
    "    print(N)\n",
    "    \n",
    "    # バッテリー充電\n",
    "    N += (b[m] - a[m])\n",
    "    print(N)\n",
    "\n",
    "N -= T - b[M - 1]\n",
    "print(N)"
   ]
