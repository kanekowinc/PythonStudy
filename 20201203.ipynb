   "source": [
    "# 入力を受け取る\n",
    "N = input()\n",
    "\n",
    "# 各桁の数の総和を求める\n",
    "result = 0\n",
    "for n in N:\n",
    "    result += int(n)\n",
    "    \n",
    "# 総和を9で割って余りが0だったら「yes」そうではない場合「no」を返す\n",
    "if result % 9 == 0:\n",
    "    print('Yes')\n",
    "else:\n",
    "    print('No')\n"
   ]
  }
